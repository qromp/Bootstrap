{
    "name": "Bootstrap",
    "tree": {
        "$className": "DataModel",

        "HttpService": {
            "$className": "HttpService",
            "$properties": {
                "HttpEnabled": true
            }
        },

        "ReplicatedStorage": {
            "$className": "ReplicatedStorage",
            "Bootstrap": {
                "$path": "src/"
            }
		},

		"ServerScriptService": {
			"$className": "ServerScriptService",
			"Example": {
				"$path": "example/"
			}
		}
    }
}
